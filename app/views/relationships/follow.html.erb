<h2>Follow Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |users| %>
    <tr>
      <td><%= attachment_image_tag users, :profile_image, :fill, 30, 30, format: 'jpeg', fallback: 'no_image.jpg' %></td>
      <td><%= users.name %></td>
      <td>フォロー数:<%= users.followings.count %></td>
      <td>フォロワー数:<%= users.followers.count %></td>
      <td><%= link_to "Show", user_path(users.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>